{
  "name": "telnyx-wdio-test-allure-docker",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf allure-results allure-report",
    "test:dev:chrome": "cross-env ENV_FILE=.env.dev wdio run ./wdio.chrome.conf.ts",
    "test:staging:chrome": "cross-env ENV_FILE=.env.staging wdio run ./wdio.chrome.conf.ts",
    "test:prod:chrome": "cross-env ENV_FILE=.env.prod wdio run ./wdio.chrome.conf.ts",
    "test:dev:firefox": "cross-env ENV_FILE=.env.dev wdio run ./wdio.firefox.conf.ts",
    "test:staging:firefox": "cross-env ENV_FILE=.env.staging wdio run ./wdio.firefox.conf.ts",
    "test:prod:firefox": "cross-env ENV_FILE=.env.prod wdio run ./wdio.firefox.conf.ts",
    "report:allure:generate": "allure generate --clean ./allure-results --output ./allure-report",
    "report:allure:open": "allure open ./allure-report"
  },
  "devDependencies": {
    "@wdio/allure-reporter": "^9.19.1",
    "@wdio/cli": "^9.19.1",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.19.1",
    "@wdio/mocha-framework": "^9.19.1",
    "@wdio/spec-reporter": "^9.19.1",
    "allure-commandline": "^2.34.1",
    "axios": "^1.11.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "expect-webdriverio": "^5.4.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.2"
  }
}
